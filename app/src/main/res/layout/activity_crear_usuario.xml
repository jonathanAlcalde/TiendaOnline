<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".crearUsuario">

    <TextView
        android:id="@+id/nom_cliente"
        android:layout_width="174dp"
        android:layout_height="19dp"
        android:layout_marginTop="84dp"
        android:text="Nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/idCliente"
        android:layout_width="174dp"
        android:layout_height="22dp"
        android:layout_marginTop="30dp"
        android:text="Id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nom_cliente" />

    <TextView
        android:id="@+id/codigoCliente"
        android:layout_width="162dp"
        android:layout_height="21dp"
        android:layout_marginTop="30dp"
        android:text="codigoCliente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idCliente" />

    <CheckBox
        android:id="@+id/terminos"
        android:layout_width="177dp"
        android:layout_height="41dp"
        android:layout_marginTop="44dp"
        android:text="Aceptar tÃ©rminos y condiciones"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/codigoCliente" />

    <Button
        android:id="@+id/registrar"
        android:layout_width="177dp"
        android:layout_height="47dp"
        android:text="REGISTRARSE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/terminos"
        app:layout_constraintVertical_bias="0.127" />
</androidx.constraintlayout.widget.ConstraintLayout>